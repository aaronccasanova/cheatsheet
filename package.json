{
	"name": "chtsht",
	"description": "Display or execute your own custom cheat sheet.",
	"version": "0.1.1",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aaronccasanova/chtsht.git"
	},
	"bugs": {
		"url": "https://github.com/aaronccasanova/chtsht/issues"
	},
	"homepage": "https://github.com/aaronccasanova/chtsht#readme",
	"bin": {
		"chtsht": "dist/cli.js",
		"chsh": "dist/cli.js"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "npm run build && dist/cli.js",
		"pretest": "npm run build",
		"test": "xo --fix"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"execa": "^5.0.0",
		"ink": "^3.2.0",
		"ink-select-input": "^4.2.1",
		"ink-table": "^3.0.0",
		"meow": "^9.0.0",
		"prompts": "^2.4.2",
		"react": "^17.0.2",
		"zx": "^4.2.0"
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"@typescript-eslint/no-non-null-assertion": "off",
			"react/prop-types": "off"
		}
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/prompts": "^2.0.14",
		"@types/react": "^17.0.38",
		"ava": "^4.0.0",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.25.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"ink-testing-library": "^2.1.0",
		"typescript": "^4.5.4",
		"xo": "^0.39.1"
	}
}
